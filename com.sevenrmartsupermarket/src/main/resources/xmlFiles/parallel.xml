<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Multiple Test" thread-count="2" parallel="tests">
	<listeners>
		<listener class-name="com.sevenrmartsupermarket.listeners.ExtentReporter"></listener>
	</listeners>
	<test name="Feature Test">
		<classes>
			<class name="com.sevenrmartsupermarket.tests.PushNotificationTest"></class>
			<class name="com.sevenrmartsupermarket.tests.ManageUsersTest"></class>
		</classes>
	</test>
	<test name="Smoke Test">
		<classes>
			<class name="com.sevenrmartsupermarket.tests.AdminUsersTest"></class>
			<class name="com.sevenrmartsupermarket.tests.PushNotificationTest"></class>
		</classes>
	</test>
</suite>